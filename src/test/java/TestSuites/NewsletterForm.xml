<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="NewsletterForm" parallel="methods" thread-count="20">
    <test name="Elements">
        <parameter name="browser" value="Firefox" />
        <classes>
            <class name="NewsletterForm.ElementsTEST.ElementsOfFormTest" />
        </classes>
    </test>
    <test name="Errors">
        <parameter name="browser" value="Firefox" />
        <classes>
            <class name="NewsletterForm.ErrorsTEST.ErrorsTest" />
        </classes>
    </test>
    <test name="Submit">
        <parameter name="browser" value="Firefox" />
        <classes>
            <class name="NewsletterForm.UserCanSubmitForm.SubmitNewsletterFormTest" />
        </classes>
    </test>
</suite>